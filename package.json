    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "autoprefixer": "^10.4.7",
    "copyfiles": "^2.4.1",
    "next": "^15.3.1",
    "next-auth": "4.18.0",
    "postcss": "^8.4.14",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.1.6",
    "typescript": "^5.8.3",
